{
  "name": "@cvplus/public-profiles",
  "version": "1.0.0",
  "description": "CVPlus Public Profiles - SEO-optimized public CV sharing and professional networking platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.esm.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./services": {
      "import": "./dist/services/index.esm.js",
      "require": "./dist/services/index.js",
      "types": "./dist/services/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.esm.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./seo": {
      "import": "./dist/seo/index.esm.js",
      "require": "./dist/seo/index.js",
      "types": "./dist/seo/index.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics/index.esm.js",
      "require": "./dist/analytics/index.js",
      "types": "./dist/analytics/index.d.ts"
    },
    "./networking": {
      "import": "./dist/networking/index.esm.js",
      "require": "./dist/networking/index.js",
      "types": "./dist/networking/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:rollup",
    "build:types": "tsc --project tsconfig.build.json",
    "build:rollup": "rollup -c",
    "build:watch": "npm run build:types -- --watch",
    "clean": "rimraf dist",
    "dev": "npm run build:watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cvplus/core": "^1.0.0",
    "@cvplus/auth": "^1.0.0",
    "@cvplus/multimedia": "^1.0.0",
    "@cvplus/premium": "^1.0.0",
    "@types/node": "^22.9.1",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.1.0",
    "react-helmet-async": "^2.0.4",
    "qrcode": "^1.5.3",
    "jsdom": "^25.0.1",
    "cheerio": "^1.0.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.4.1",
    "node-cache": "^5.1.2",
    "validator": "^13.12.0",
    "slug": "^9.1.0",
    "mime-types": "^2.1.35"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/qrcode": "^1.5.5",
    "@types/jsdom": "^21.1.7",
    "@types/cheerio": "^0.22.35",
    "@types/validator": "^13.12.2",
    "@types/slug": "^5.0.8",
    "@types/mime-types": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@testing-library/react": "^14.3.0",
    "@testing-library/jest-dom": "^6.4.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "cvplus",
    "public-profiles",
    "seo-optimization",
    "professional-networking",
    "cv-sharing",
    "portfolio-showcase",
    "social-integration",
    "personal-branding",
    "analytics-tracking",
    "custom-domains",
    "typescript",
    "react"
  ],
  "author": "Gil Klainert",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvplus/cvplus.git",
    "directory": "packages/public-profiles"
  },
  "bugs": {
    "url": "https://github.com/cvplus/cvplus/issues"
  },
  "homepage": "https://github.com/cvplus/cvplus/tree/main/packages/public-profiles#readme"
}